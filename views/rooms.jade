extends layout

block header
    link(rel='stylesheet', href='/stylesheets/rooms.css')
    script(src="/javascripts/jquery-2.1.1.min.js", type='text/javascript')
    script(src="/socket.io/socket.io.js", type="text/javascript")
    script(src="/javascripts/rooms.js", type='text/javascript')
block content
    h1 Room page
    p#btn Welcome to Room #{id}
    p= counter
    div#main
        img#box.gui(src="/images/IconBox.png")
        img#hot.gui(src="/images/HotIcon.png")
        img#cold.gui(src="/images/ColdIcon.png")
